{
  "name": "sorbus",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-message": "babel-node config/startMessage.js",
    "prestart": "npm run clean-cache",
    "start": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\"",
    "open:src": "babel-node config/srcServer.js",
    "open:dist": "babel-node config/distServer.js",
    "lint": "esw src config --ext js,jsx",
    "lint:watch": "npm run lint -- --watch",
    "clean-cache": "rimraf node_modules/.cache",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run clean-dist",
    "build": "babel-node config/build.js && npm run open:dist",
    "generate": "babel-node src/utils/generateFakeTasks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tenemo/Sorbus.git"
  },
  "author": "tenemo@gmail.com",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Tenemo/Sorbus/issues"
  },
  "homepage": "https://github.com/Tenemo/Sorbus#readme",
  "dependencies": {
    "connected-react-router": "^4.4.1",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.9",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-immutable-pure-component": "^1.2.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "sanitize.css": "^6.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "browser-sync": "^2.24.6",
    "casual": "^1.5.19",
