{
    "name": "sorbus",
    "version": "0.1.4",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start-message": "babel-node config/startMessage.js",
        "prestart": "npm run clean-cache && npm run start-message",
        "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
        "open:src": "babel-node config/srcServer.js",
        "open:dist": "babel-node config/distServer.js",
        "lint": "esw src config --ext js,jsx",
        "lint:watch": "npm run lint -- --watch",
        "clean-cache": "rimraf node_modules/.cache",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "prebuild": "npm run lint && npm run test && npm run clean-dist",
        "build": "babel-node config/build.js && npm run open:dist",
        "test": "jest",
        "test:watch": "npm run test -- --watch",
        "test:coverage": "npm run test -- --coverage",
        "generate": "babel-node src/utils/generateFakeTasks"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tenemo/Sorbus.git"
    },
    "author": "tenemo@gmail.com",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/Tenemo/Sorbus/issues"
    },
    "homepage": "https://github.com/Tenemo/Sorbus#readme",
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/config/enzymeSetup.js",
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/assetsTransformer.js",
            "^.+\\.(css|scss)$": "<rootDir>/config/assetsTransformer.js"
        },
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testEnvironment": "node"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "connected-react-router": "^4.4.1",
        "history": "^4.7.2",
        "immutable": "^4.0.0-rc.9",
        "prop-types": "^15.6.2",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.0",
        "redux-immutable": "^4.0.0",
        "sanitize.css": "^6.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^9.1.1",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.6",
        "babel-loader": "^7.1.5",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-plugin-transform-react-jsx-source": "^6.22.0",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "browser-sync": "^2.24.6",
        "casual": "^1.5.19",
        "chalk": "^2.4.1",
        "concurrently": "3.5.0",
        "connect-history-api-fallback": "^1.5.0",
        "css-loader": "^1.0.0",
        "enzyme": "^3.4.4",
        "enzyme-adapter-react-16": "^1.2.0",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^5.4.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-import-resolver-webpack": "^0.10.1",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jest": "^21.21.0",
        "eslint-plugin-jsdoc": "^3.7.1",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-react": "^7.11.1",
        "eslint-watch": "^4.0.2",
        "eventsource-polyfill": "^0.9.6",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^4.3.1",
        "jest": "^23.5.0",
        "jest-cli": "^23.5.0",
        "node-sass": "^4.9.3",
        "npm": "^6.4.0",
        "postcss-loader": "^3.0.0",
        "react-hot-loader": "^4.3.4",
        "redux-devtools-extension": "^2.13.5",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.22.1",
        "webpack": "^4.16.5",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-hot-middleware": "^2.22.3"
    }
}
